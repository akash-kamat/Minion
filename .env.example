# Minion Configuration
# Copy this to .env and fill in your values

# Your Aternos server address (e.g., yourserver.aternos.me)
MC_HOST=yourserver.aternos.me

# Server port (shown on Aternos dashboard when online)
MC_PORT=25565

# Your minion's name (anything you want for cracked servers)
MC_USERNAME=Minion

# Minecraft version (check your server version)
MC_VERSION=1.21.1

# API server port (for the control panel)
API_PORT=3000
